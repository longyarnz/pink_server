{
  "version": 2,
  "name": "pink_et_tu",
  "alias": ["api.pinkettu.com.ng"],
  "env": {
    "PORT": "3001",
    "HOST": "0.0.0.0",
    "MONGODB": "@prod-db",
    "MAIL_PASS": "@mail-pass",
    "HASH": "10",
    "PAYSTACK": "@paystack_test_key",
    "SERVER_KEY": "function(UUID){return UUID.v4()}",
    "REFRESH_TOKEN": "@refreshtoken",
    "ACCESS_TOKEN": "@accesstoken",
    "CLIENT_ID": "@clientid",
    "CLIENT_SECRET": "@clientsecret"
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}